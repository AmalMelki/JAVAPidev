<AnchorPane id="AnchorPane" prefHeight="76.0" prefWidth="995.0" style="-fx-background-color: white;" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.pidevuser.gui.FXML1Controller">
   <children>
      <HBox fx:id="HboxProg" prefHeight="81.0" prefWidth="1000.0" style="-fx-background-color: #A98C78;" stylesheets="@style.css">
         <children>
            <Label fx:id="lbtitre" alignment="CENTER" prefHeight="64.0" prefWidth="93.0" text="Titre" />
            <Label fx:id="lbdescription" alignment="CENTER" prefHeight="64.0" prefWidth="120.0" text="Description" />
            <Label fx:id="lbadresse" alignment="CENTER" prefHeight="64.0" prefWidth="95.0" text="Adresse" />
            <Label fx:id="lbregion" alignment="CENTER" prefHeight="64.0" prefWidth="99.0" text="Region" />
            <Label fx:id="lbdate" alignment="CENTER" prefHeight="65.0" prefWidth="92.0" text="Date" />
            <Label fx:id="lbprix" alignment="CENTER" prefHeight="65.0" prefWidth="92.0" text="Prix" />
            <Label fx:id="lbguide" alignment="CENTER" prefHeight="65.0" prefWidth="92.0" text="Guide" textAlignment="CENTER" />
            <Label fx:id="lbcategorie" prefHeight="65.0" prefWidth="92.0" text="Categorie" />
            <Label fx:id="lbtransport" prefHeight="65.0" prefWidth="92.0" text="Transport" />
            <ImageView fx:id="imgProg" fitHeight="56.0" fitWidth="66.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@about.jpg" />
               </image>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </ImageView>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#Afficher" prefHeight="37.0" prefWidth="77.0" styleClass="add-btn" stylesheets="@style.css" text="Afficher" textAlignment="CENTER">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
               </HBox.margin>
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#supprimer" prefHeight="37.0" prefWidth="100.0" styleClass="add-btn" stylesheets="@style.css" text="Supprimer" textAlignment="CENTER">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
               </HBox.margin>
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
